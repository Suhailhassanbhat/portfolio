ex-wrap: wrap;\n}\n\n/* Everything inside of scrollytelling gets normal full width... */\n.scrollytelling > * {\n  flex-basis: 100%;\n  flex-shrink: 0;\n}\n\n/* ...except the .steps and .chart! They'll share a row, sometimes */\n.scrollytelling > .steps,\n.scrollytelling > .chart {\n  flex-basis: 20%;\n  flex-shrink: 0;\n  flex-grow: 1;\n}\n\n/* The bigger 'flex-grow' the more space it takes up */\n.scrollytelling > .chart {\n  flex-grow: 3;\n  overflow: hidden;\n}\n\n.scrollytelling .chart {\n  margin-right: 15px;\n  position: sticky;\n  top: 0;\n  z-index: 0;\n  height: 100vh;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.scrollytelling .chart svg {\n  margin: auto;\n}\n\n.scrollytelling .chart img {\n  width: 100wh;\n}\n\n.scrollytelling .step {\n  max-width: 30rem;\n  margin: 0 auto;\n  margin-bottom: 80vh;\n  padding: 30px;\n  background: rgba(255, 255, 255, 0.90);\n  pointer-events: auto;\n}\n#temp1 {\n  background-color: #e41a1c;\n  color: white;\n}\n#temp2 {\n  background-color: #377eb8;\n  color: white;\n}\n#temp3 {\n  background-color:#4daf4a;\n  color: white;\n}\n#temp4 {\n  background-color:#984ea3;\n  color: white;\n}\n#temp5 {\n  background-color:#ff7f00;\n  color: white;\n}\n.scrollytelling .step *:first-child {\n  margin-top: 0;\n  padding-top: 0;\n}\n\n.scrollytelling .step *:last-child {\n  margin-bottom: 0;\n  padding-bottom: 0;\n}\n\n/* This is a weird spacing fix */\n.scrollytelling .steps {\n  z-index: 10;\n  padding-bottom: 1vh;\n  pointer-events: none;\n}\n\n/* This is the point where it collapses */\n@media (max-width:900px) {\n  .scrollytelling > * {\n      flex-basis: 100% !important;\n  }\n  .scrollytelling .chart {\n      margin-right: 0;\n  }\n}\n\n\n\n/* \n\nEnd scrollytelling \n\n*/"},"lineCount":null}},"error":null,"hash":"8d00b6b3d04cb4b950f1c781b5594c49","cacheData":{"env":{}}}